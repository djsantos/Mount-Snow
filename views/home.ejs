<!DOCTYPE html>
<html>
	<head><title><%= title %></title></head>
	<body>
		<header>
			<nav>
				<ul>
				  <li><a href = "/home">Home</a></li>
				  <li><a href = "/connect">Connect</a></li>
				  <li><a href = "/discover">Discover</a></li>
				  <li><a href = "/me">Me</a></li>
				  <li><h1>Twitter</h1>
				  <li><a href = "/help">Help</a></li>
				  <li><a href = "/search">Search</a></li>
				  <li><a href = "#">Options</a></li>
				  <li><a href = "#">Compose</a></li>
				</ul>
			</nav>
		</header>
		<aside>
			<ul>
				<li><a href = "/home"><%=username%></a></li>
				<!-- href must be changed to the me subpage tweets when the subpage is ready -->
				<li><a href = "/me"><%=tweets%> Tweets</a></li>
				<!-- href must be changed to the me subpage following when the subpage is ready -->
				<li><a href = "/me"><%=following%> Following</a></li>
				<!-- href must be changed to the me subpage followers when the subpage is ready -->
				<li><a href = "/me"><%=followers%> Followers</a></li>
			</ul>
		</aside>
		<section id = "TwitterFeed">
			<h2>Tweet Feed</h2>
			<%for(var i = 0; i< 60; i = i+3){%>
				<header class="postHeader">
					<h5 class="postAlias"><a href="#"><%=feed[i]%></a></h5>
					<span class="postTimestamp"><%=feed[i+1]%></span>
					</header>
					<div class="postMessage">
					<p><%=feed[i+2]%></p>
					</div>
					</div>
				<%}%>
		</section>
	</body>
</html>
