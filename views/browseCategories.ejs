		<div id = 'TwitterFeed'>
			<ul>
				<h2>Browse Categories <%=username%></h2>
				<%for(var i = 0; i< tags.length; i = i + 3){%>
					<li>
						<h3>#<%=tags[i]%>  <%=tags[i+1]%></h3>
						<p><%=tags[i+2]%></p>
					</li>
				<%}%>
			</ul>
		</div>
		</div>
	</body>
</html>
